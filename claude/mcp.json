{
    "mcpServers": {
        "mobile": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@mobilenext/mobile-mcp@latest"
            ],
            "env": {}
        },
        "playwright": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@playwright/mcp@latest"
            ],
            "env": {}
        },
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp"
        }
    }
}