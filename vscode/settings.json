{
    "chat.agent.enabled": true,
    "chat.commandCenter.enabled": false,
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.experimental.showMoves": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.codeActionsOnSave": {
        "source.action.useSortedKeys.biome": "explicit",
        "source.fixAll.biome": "explicit",
        "source.organizeImports": "explicit",
        "source.removeUnusedImports": "explicit",
    },
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.experimental.asyncTokenization": true,
    "editor.formatOnPaste": true,
    "editor.guides.bracketPairs": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.lineHeight": 1.9,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.stickyScroll.enabled": true,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
        "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.test.ts,${capture}.spec.ts,${capture}.js",
        "*.tsx": "${capture}.test.tsx,${capture}.spec.tsx,${capture}.ts",
        "*.jsx": "${capture}.test.jsx,${capture}.spec.jsx,${capture}.js",
        "*.js": "${capture}.test.js,${capture}.spec.js,${capture}.js.map, ${capture}.min.js, ${capture}.d.ts"
    },
    "emmet.includeLanguages": {
        "erb": "html",
    },
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "git.confirmSync": false,
    "gitlab.duoAgentPlatform.enabled": false,
    "gitlab.duoChat.enabled": false,
    "gitlab.duoCodeSuggestions.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "material-icon-theme.activeIconPack": "qwik",
    "rubyLsp.formatter": "rubocop",
    "rubyLsp.rubyVersionManager": "rbenv",
    "screencastMode.keyboardOptions": {
        "showSingleEditorCursorMoves": false
    },
    "settingsSync.ignoredExtensions": [
        "ms-vscode-remote.remote-wsl"
    ],
    "telemetry.telemetryLevel": "off",
    "vsintellicode.sql.completionsEnabled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.newWindowProfile": "Default",
    "workbench.activityBar.location": "top",
    "workbench.colorTheme": "Dracula Theme",
    "workbench.editor.tabSizingFixedMaxWidth": 120,
    "workbench.iconTheme": "material-icon-theme",
}